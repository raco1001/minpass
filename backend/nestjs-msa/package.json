    "@nestjs/schematics": "^11.0.0",
    "@nestjs/testing": "^11.0.1",
    "@sqnest/drizzle": "^0.1.0",
    "@swc/cli": "^0.6.0",
    "@swc/core": "^1.10.7",
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.10.7",
