{
  "name": "minpass",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "adl:new": "sh scripts/new-dr.sh",
    "adl:index": "node scripts/gen-adl-index.mjs",
    "adl:index:flat": "node scripts/gen-adl-index.mjs --group=none --sort=created --order=desc",
    "adl:index:status": "node scripts/gen-adl-index.mjs --group=status --sort=updated --order=desc",
    "build": "nest build",
    "build:dev": "nest build apis --watch",
    "build:prod": "nest build apis",
    "build:users": "nest build users",
    "build:auth": "nest build auth",
    "build:apis": "nest build apis",
    "build:activities": "nest build activities",
    "build:journals": "nest build journals",
    "build:notifications": "nest build notifications",
    "build:ontology": "nest build ontology",
    "build:schedules": "nest build schedules",
    "build:tasks": "nest build tasks",
    "format": "prettier --write \"apps/**/*.ts\" \"libs/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start apis --watch",
    "start:debug": "nest start apis --debug --watch",
    "start:prod": "node dist/apps/apis/main.js",
    "start:apis": "nest start apis --watch",
    "start:auth": "nest start auth --watch",
    "start:users": "nest start users --watch",
    "start:activities": "nest start activities --watch",
    "start:journals": "nest start journals --watch",
    "start:notifications": "nest start notifications --watch",
    "start:ontology": "nest start ontology --watch",
    "start:schedules": "nest start schedules --watch",
    "start:tasks": "nest start tasks --watch",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:auth": "jest --config apps/auth/test/jest.config.js",
    "test:auth:watch": "jest --config apps/auth/test/jest.config.js --watch",
    "test:auth:cov": "jest --config apps/auth/test/jest.config.js --coverage",
    "test:auth:e2e": "jest --config apps/auth/test/jest-e2e.config.js",
    "proto:clean": "rimraf libs/contracts/generated/*",
    "proto:gen": "protoc --plugin=protoc-gen-ts_proto=./node_modules/.bin/protoc-gen-ts_proto --ts_proto_out=libs/contracts/src/generated --ts_proto_opt=nestJs=true --ts_proto_opt=stringEnums=true --proto_path=./proto $(find proto -type f -name \"*.proto\" -print)",
    "proto:watch": "chokidar \"libs/contracts/proto/**/*.proto\" -c \"pnpm proto:gen\"",
    "auth:seed-providers": "tsx apps/auth/scripts/seed-providers.ts"
  },
