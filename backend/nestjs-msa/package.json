    "mysql2": "^3.15.0",
    "neo4j-driver": "^5.28.2",
    "nest-neo4j": "^0.3.1",
    "nestjs-pino": "^4.4.0",
    "passport-github2": "^0.1.12",
    "passport-google-oauth20": "^2.0.0",
    "passport-kakao": "^1.0.1",
    "pino-http": "^10.5.0",
    "pino-pretty": "^13.1.1",
    "protobufjs": "^7.5.4",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
